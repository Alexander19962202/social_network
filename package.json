{
  "name": "sowa-web-client",
  "version": "0.9.0",
  "author": "AlexEvs",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build:dev": "PUBLIC_URL=/ react-scripts build",
    "serve": "serve -s build",
    "test": "react-scripts test",
    "test:coverage": "react-scripts test --coverage --runInBand --watchAll=false",
    "format": "prettier --write \"src/**/*.{ts,tsx,css,scss}\"",
    "lint": "eslint \"src/**/*.{ts,tsx}\"",
    "lint:fix": "eslint \"src/**/*.{ts,tsx}\" --fix",
    "check-deps": "yarn outdated",
    "upgrade-deps": "yarn upgrade --latest",
    "build:prod": "react-scripts build",
    "predeploy": "yarn run build:prod",
    "deploy": "gh-pages -d build -m 'Release <version>' -n",
    "prepare": "husky install",
    "eject": "react-scripts eject"
  },
  "dependencies": {
    "@reduxjs/toolkit": "1.9.5",
    "@sentry/react": "7.57.0",
    "axios": "1.4.0",
    "classnames": "2.3.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-redux": "8.1.1",
    "react-router-dom": "6.14.1",
    "react-scripts": "5.0.1",
    "redux-form": "8.3.10",
    "sass": "1.63.6",
    "web-vitals": "3.3.2"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "7.21.11",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "14.0.0",
    "@testing-library/user-event": "14.4.3",
    "@types/axios": "0.14.0",
    "@types/classnames": "2.3.1",
    "@types/jest": "29.5.2",
    "@types/react": "18.2.14",
    "@types/react-dom": "18.2.6",
    "@types/react-redux": "7.1.25",
    "@types/react-router-dom": "5.3.3",
    "@types/react-test-renderer": "18.0.0",
    "@types/redux": "3.6.0",
    "@types/redux-form": "8.3.5",
    "@types/redux-thunk": "2.1.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-import-resolver-typescript": "3.5.5",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-unused-imports": "2.0.0",
    "gh-pages": "5.0.0",
    "husky": "8.0.3",
    "jest-mock-axios": "4.7.2",
    "lint-staged": "13.2.3",
    "prettier": "2.8.8",
    "react-test-renderer": "18.2.0",
    "serve": "14.2.0",
    "typescript": "5.1.6"
  }
}
